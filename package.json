{
	"author": "T. Jameson Little <t.jameson.little@gmail.com>",
	"name": "filesync-client",
	"description": "Client for FileSync, a file synchronization platform",
	"version": "0.0.1",
	"repository": {
		"url": ""
	},
	"main": "bin/filesync",
	"config": {
		"init-system": "systemd",
		"port": 2500,
		"pam-module": true,
		"pam-module-name": "http-auth.so",
		"pam-module-path": "."
	},
	"engines": {
		"node": "~0"
	},
	"scripts": {
		"preinstall": "./configure",
		"install": "./install",
		"start": "bin/filesync &",
		"stop": "curl -X PUT http://localhost:2500/stop"
	},
	"dependencies": {
		"connect": "~1",
		"tar-async": "~1",
		"optimist": "~0",
		"node-fs": "*",
		"inotify-plusplus": "~1",
		"eventhub": "~1"
	},
	"devDependencies": {},
	"preferGlobal": true
}
